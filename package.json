{
  "name": "cultureamp-front-end-scripts",
  "version": "0.0.0-development",
  "main": "index.js",
  "repository": "https://github.com/cultureamp/cultureamp-front-end-scripts.git",
  "author": "FECT",
  "license": "MIT",
  "bin": {
    "cultureamp-front-end-scripts-start": "./scripts/start.sh",
    "cultureamp-front-end-scripts-build": "./scripts/build.sh",
    "cultureamp-front-end-scripts-test": "./scripts/test.sh",
    "cultureamp-front-end-scripts-lint": "./scripts/lint.sh",
    "cultureamp-front-end-scripts-flow": "./scripts/flow.sh",
    "cultureamp-front-end-scripts-format": "./scripts/format.sh"
  },
  "scripts": {
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "footer-leading-blank": [
        0,
        "always"
      ],
      "header-max-length": [
        0,
        "always"
      ],
      "subject-case": [
        0,
        "always"
      ]
    }
  },
  "release": {
    "preset": "angular",
    "parserOpts": {
      "noteKeywords": [
        "BREAKING CHANGE",
        "BREAKING CHANGES",
        "BREAKING",
        "ðŸ’”"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-jest": "^22.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-modules-loader-core2": "^0.0.1",
    "cultureamp-style-guide": "^18.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.73.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.3",
    "react-hot-loader": "^4.0.1",
    "svg-sprite-loader": "3.7.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.0-rc.9",
    "semantic-release": "^15.6.0"
  }
}
